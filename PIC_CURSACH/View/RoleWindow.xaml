<Window x:Class="PIC_CURSACH.View.RoleWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:PIC_CURSACH.View"
        xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
        mc:Ignorable="d"
        Title="RoleWindow" Height="450" Width="800">
    <Grid Background="{DynamicResource WindowBackground}">
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock x:Name="RoleText" FontWeight="Bold" FontSize="22" Margin="0 0 0 20" HorizontalAlignment="Center"/>
            <ui:Button Content="Выйти" Click="Button_Logout"/>
        </StackPanel>
    </Grid>
</Window>
